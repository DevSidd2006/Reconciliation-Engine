        "composite": false,
        "clientRole": false,
        "containerId": "reconciliation"
      },
      {
        "name": "viewer",
        "description": "Read-only access to basic data",
        "composite": false,
        "clientRole": false,
        "containerId": "reconciliation"
      }
    ]
  },
